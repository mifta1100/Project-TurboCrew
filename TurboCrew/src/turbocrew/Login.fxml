<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="scenePane" prefHeight="480.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="turbocrew.LoginController">
   <children>
      <Label fx:id="headerLabel" alignment="CENTER" layoutY="28.0" prefHeight="32.0" prefWidth="600.0" text="Login">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="headerTextLabel" alignment="CENTER" layoutY="75.0" prefHeight="32.0" prefWidth="600.0" text="">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" layoutX="74.0" layoutY="130.0" prefHeight="27.0" prefWidth="459.0" text="Username">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="passwordLabel" layoutX="74.0" layoutY="227.0" prefHeight="27.0" prefWidth="459.0" text="Password">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="74.0" layoutY="173.0" prefHeight="32.0" prefWidth="459.0" />
      <PasswordField fx:id="passwordField" layoutX="74.0" layoutY="267.0" prefHeight="32.0" prefWidth="459.0" />
      <Button fx:id="bigButton" layoutX="74.0" layoutY="332.0" mnemonicParsing="false" onAction="#bigButtonAction" prefHeight="32.0" prefWidth="459.0" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <HBox layoutX="74.0" layoutY="387.0" prefHeight="42.0" prefWidth="459.0" spacing="20.0">
         <children>
            <Label fx:id="varLabel" prefHeight="42.0" text="Need an account?" />
            <Button fx:id="smallButton" mnemonicParsing="false" onAction="#smallButtonAction" prefHeight="43.0" text="Register" textAlignment="CENTER" />
         </children>
      </HBox>
   </children>
</AnchorPane>